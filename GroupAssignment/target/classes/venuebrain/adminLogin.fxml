<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="venuebrain.adminLoginController">
    <children>
      <AnchorPane prefHeight="517.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="63.0" layoutY="44.0" text="Venue Brain Admin Login">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="43.0" layoutY="410.0" mnemonicParsing="false" onAction="#btnBackWasClicked" text="&lt;&lt; Back" />
              <Button layoutX="472.0" layoutY="410.0" onAction="#switchToPrimary" text="Test addNewGuest" />
            <Label layoutX="123.0" layoutY="186.0" text="Administrator Username:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="adminUsername" layoutX="290.0" layoutY="182.0" />
            <Button fx:id="loginButton" layoutX="266.0" layoutY="296.0" mnemonicParsing="false" onAction="#btnLoginClicked" text="Login" />
            <Label fx:id="invalidLogin" layoutX="290.0" layoutY="249.0" text="Username or password invalid!" textFill="RED">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label layoutX="125.0" layoutY="218.0" text="Administrator Password:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <PasswordField fx:id="adminPassword" layoutX="290.0" layoutY="214.0" />
         </children>
      </AnchorPane>
    </children>
</VBox>
