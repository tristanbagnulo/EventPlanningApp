<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="venuebrain.PrimaryController">
   <children>
      <AnchorPane prefHeight="517.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="guestButton" layoutX="63.0" layoutY="257.0" mnemonicParsing="false" onAction="#btnGuestWasClicked" prefHeight="34.0" prefWidth="160.0" text="Guest Login">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <HBox layoutX="346.0" layoutY="57.0" prefHeight="100.0" prefWidth="200.0" />
            <Button fx:id="adminButton" layoutX="418.0" layoutY="257.0" onAction="#switchToAdminLogin" prefHeight="34.0" prefWidth="160.0" text="Administrator Login" />
            <Label layoutX="63.0" layoutY="126.0" text="Welcome to Venue Brain">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
