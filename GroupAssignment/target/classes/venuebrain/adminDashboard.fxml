<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="640.0" prefWidth="950.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="venuebrain.adminDashboardController">
   <children>
      <AnchorPane prefHeight="578.0" prefWidth="764.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField layoutX="204.0" layoutY="529.0" prefHeight="33.0" prefWidth="165.0" promptText="Event ID" />
            <Label layoutX="286.0" layoutY="57.0" text="Admin Dashboard">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="24.0" layoutY="592.0" mnemonicParsing="false" onAction="#btnBackWasClicked" text="&lt;&lt; Back" />
            <TableView layoutX="195.0" layoutY="130.0" prefHeight="379.0" prefWidth="583.0">
              <columns>
                <TableColumn editable="false" prefWidth="193.0" text="Event ID" />
                <TableColumn prefWidth="207.0" text="Name" />
                  <TableColumn prefWidth="182.0" text="Location" />
              </columns>
            </TableView>
            <Button layoutX="794.0" layoutY="533.0" mnemonicParsing="false" text="Create" />
            <Button layoutX="819.0" layoutY="586.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="51.0" text="Edit" />
            <BorderPane layoutY="3.0" prefHeight="122.0" prefWidth="950.0">
               <top>
                  <MenuBar fx:id="adminMenu" BorderPane.alignment="CENTER">
                    <menus>
                      <Menu fx:id="newMenu" mnemonicParsing="false" text="New">
                        <items>
                          <MenuItem fx:id="newEvent" mnemonicParsing="false" text="New Event" />
                              <MenuItem fx:id="newGuest" mnemonicParsing="false" text="New Guest" />
                        </items>
                      </Menu>
                      <Menu fx:id="editMenu" mnemonicParsing="false" text="Edit">
                        <items>
                          <MenuItem fx:id="editEvent" mnemonicParsing="false" text="Edit Event" />
                              <MenuItem fx:id="deleteGuest" mnemonicParsing="false" text="Delete Guest" />
                        </items>
                      </Menu>
                      <Menu fx:id="helpMenu" mnemonicParsing="false" text="Help">
                        <items>
                          <MenuItem fx:id="about" mnemonicParsing="false" onAction="#showAbout" text="About" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
               </top>
            </BorderPane>
            <TextField layoutX="604.0" layoutY="529.0" prefHeight="33.0" prefWidth="165.0" promptText="Location" />
            <TextField layoutX="404.0" layoutY="529.0" prefHeight="33.0" prefWidth="165.0" promptText="Name" />
         </children>
      </AnchorPane>
   </children>
</VBox>
