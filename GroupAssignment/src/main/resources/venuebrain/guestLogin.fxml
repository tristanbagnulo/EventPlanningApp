<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="venuebrain.guestLoginController">
   <children>
      <AnchorPane VBox.vgrow="ALWAYS">
         <children>
            <HBox layoutX="346.0" layoutY="57.0" prefHeight="100.0" prefWidth="200.0" />
            <Label layoutX="63.0" layoutY="44.0" text="Venue Brain Guest Login">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Label layoutX="93.0" layoutY="217.0" text="Please enter your Guest Access Code:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="guestAccess" layoutX="339.0" layoutY="213.0" />
            <Button fx:id="loginButton" layoutX="322.0" layoutY="309.0" mnemonicParsing="false" onAction="#btnLoginClicked" text="Login" />
            <Button fx:id="backButton" layoutX="43.0" layoutY="410.0" mnemonicParsing="false" onAction="#btnBackWasClicked" text="&lt;&lt; Back" />
            <Label fx:id="invalidCode" layoutX="339.0" layoutY="248.0" text="Guest Access Code is not valid!" textFill="RED">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
